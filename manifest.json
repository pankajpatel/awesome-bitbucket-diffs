{
  "manifest_version": 2,
  "name": "Awesome Bitbucket Diffs",
  "description": "Awesome collapsible diffs for BitBucket",
  "version": "0.1",
  "homepage_url": "https://time2hack.com/extensions/awesome-bitbucket-diffs",
  "browser_action": {
    "default_icon": "icons/diff-128.png",
    "default_title": "Awesome Bitbucket Diffs"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["http://bitbucket.org/*", "https://bitbucket.org/*"],
    "js": ["contentScript.js"]
  }],
  "icons": {
    "16": "icons/diff-16.png",
    "48": "icons/diff-48.png",
    "128": "icons/diff-128.png"
  }
}
